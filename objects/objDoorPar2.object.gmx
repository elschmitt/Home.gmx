        var push = 0;
    var mouseRight = 0;
    if (mouse_check_button_pressed(mb_right) &amp;&amp; position_meeting(mouse_x,mouse_y,self)) {
        mouseRight = 1;
        }
    if (mouse_check_button_pressed(mb_left) &amp;&amp; position_meeting(mouse_x,mouse_y,self)) {
