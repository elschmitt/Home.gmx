            }
            diag_show(0,whatToSay,id,ev_user0);
        }
    // Examine Usable Object
    if (usable == 1) &amp;&amp; push == 1 &amp;&amp; (place_meeting(x,y,objPlayer)) {
            if globalSetting == 0 {
                diag_show(0,whatToSay,id,ev_user1);
                if (sprite_index == swapTo) {
                    if useSound != sndNothing {
                    audio_play_sound(useSound,1,0);
