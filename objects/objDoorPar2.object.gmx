            mouseRight = 1;
            }
        }
    push = max(keyboard_check_pressed(global.xKey),gamepad_button_check_pressed(global.whichPad,global.xButton),mouseRight,0);
        if push == 1 {
            if locked != 99 {
                if toUnlockMe != 99 {
                    // If Door has been unlocked, and can now be accessed
                    if toUnlockMe == 1 {
                        if locked == 1 {
                            // For factory card door, etc.
                            if confirmMe == 1 {
                                if whatToSayFirst != "99" {
                                    diag_show(0,whatToSayFirst,id,ev_user0);
                                    }
                                if whatToSayFirst == "99" {
                                    diag_show(1,whatToSayAgain,id,ev_user1);
